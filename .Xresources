XClock*analog: false
XClock*face: Cozette:pixelsize=13:style=Medium
XClock*geometry: 130x25+895+1
XClock*padding: 4
XClock*background: #333333
XClock*foreground: #ffffff

XTerm*title: terminal
XTerm.termName: xterm-256color

XTerm*vt100.geometry: 100x30
XTerm*internalBorder: 20
XTerm*saveLines: 30000

XTerm*faceName: PragmataPro Mono
XTerm.vt100.allowBoldFonts: false
XTerm.vt100.faceSize: 10.5

XTerm.vt100.faceSize1: 9
XTerm.vt100.faceSize2: 10.5
XTerm.vt100.faceSize3: 12
XTerm.vt100.faceSize4: 14
XTerm.vt100.faceSize5: 16
XTerm.vt100.faceSize6: 20
XTerm.vt100.faceSize7: 24

XTerm*selectToClipboard: true
XTerm*highlightSelection: true
XTerm*trimSelection: true
XTerm*metaSendsEscape: true

! keybindings
XTerm*vt100.translations: #override \n\
  Ctrl <Key> minus: smaller-vt-font() \n\
  Ctrl <Key> plus: larger-vt-font() \n\
  Ctrl Shift <Key> c: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key> v: insert-selection(CLIPBOARD)

! colors
XTerm*background: #1D262A
XTerm*foreground: #E7EBED

! black
XTerm*color0: #333C41
XTerm*color8: #84A6B8

! red
XTerm*color1: #FC3841
XTerm*color9: #FC746D

! green
XTerm*color2: #5CF19E
XTerm*color10: #ADF7BE

! yellow
XTerm*color3: #FED032
XTerm*color11: #FEE16C

! blue
XTerm*color4: #37B6FF
XTerm*color12: #70CFFF

! magenta
XTerm*color5: #FC226E
XTerm*color13: #FC669B

! cyan
XTerm*color6: #59FFD1
XTerm*color14: #9AFFE6

! white
XTerm*color7: #FFFFFF
XTerm*color15: #FFFFFF
